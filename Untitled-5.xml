                          
                          <ns34:externalId>
                          <xsl:value-of select="$AcctRec/nsmpr1:AccountList/nsmpr1:externalId" />
                          </ns34:externalId>
                          <ns34:acctNumber>
                                <xsl:value-of select="$AcctRec/nsmpr1:AccountList/nsmpr1:AcctNunber"/>
                          </ns34:acctNumber>
                          <ns34:acctName>
                                <xsl:value-of select="substring($AcctRec/nsmpr1:AccountList/nsmpr1:AcctName,1,60)"/>
                          </ns34:acctName>
                          <ns34:acctType>
                                <xsl:value-of select="dvm:lookupValue (&quot;tenant/resources/dvms/LICY-ERP-FIN-CAT-NS-ACCT-TYPE&quot;, &quot;ERP_FIN_CATAGORIES&quot;, $AcctRec/nsmpr1:AccountList/nsmpr1:FinancialCatagory, &quot;NS_ACCOUNT_TYPE&quot;, '&quot;none, valid field required in ERP to Netsuite Lookup Table&quot;' )"/>
                          </ns34:acctType>
                          <xsl:variable name="netsuiteValue" select="dvm:lookupValue (&quot;tenant/resources/dvms/LICY-ERP-FIN-CAT-NS-ACCT-TYPE&quot;, &quot;ERP_FIN_CATAGORIES&quot;, $AcctRec/nsmpr1:AccountList/nsmpr1:FinancialCatagory, &quot;NS_ACCOUNT_TYPE&quot;, '&quot;none, valid field required in ERP to Netsuite Lookup Table&quot;' )"/>
                          <xsl:variable name="ERPValue" select="$AcctRec/nsmpr1:AccountList/nsmpr1:FinancialCatagory"/>
                          <xsl:choose>
                                <xsl:when test="$AcctRec/nsmpr1:AccountList/nsmpr1:EnabledFlag = &quot;N&quot;">
                                      <ns34:isInactive>
                                            <xsl:value-of select="true ()"/>
                                      </ns34:isInactive>
                                </xsl:when>
                                <xsl:otherwise>
                                      <ns34:isInactive>
                                            <xsl:value-of select="false ()"/>
                                      </ns34:isInactive>
                                </xsl:otherwise>
                          </xsl:choose>
                          <xsl:choose>
                                <xsl:when test="$netsuiteValue = &quot;_bank&quot;">
                                      <ns34:currency>
                                            <xsl:choose>
                                                  <xsl:when test="$ERPValue = &quot;Cash USD&quot;">
                                                        <ns35:name>
                                                              <xsl:value-of select="&quot;US Dollar&quot;"/>
                                                        </ns35:name>
                                                  </xsl:when>
                                                  <xsl:otherwise>
                                                        <ns35:name>
                                                              <xsl:value-of select="&quot;Canadian Dollar&quot;"/>
                                                        </ns35:name>
                                                  </xsl:otherwise>
                                            </xsl:choose>
                                      </ns34:currency>
                                      <!--<ns34:currency internalId="1"/>-->
                                      <ns34:subsidiaryList>
                                            <ns35:recordRef internalId="8"/>
                                      </ns34:subsidiaryList>
                                </xsl:when>
                                <xsl:otherwise>
                                      <ns34:subsidiaryList>
                                            <xsl:for-each select="$AcctRec/nsmpr1:AccountList/nsmpr1:SubsidiaryList">
                                                  <ns35:recordRef internalId="{nsmpr1:SubsidiaryInternalId}"/>
                                            </xsl:for-each>
                                      </ns34:subsidiaryList>
                                </xsl:otherwise>
                          </xsl:choose>